{
	"name": "Tab Finder",
	"description": "Find a tab with some search conditions",
	"version": "1.0",
	"browser_action": {
		"default_popup": "popup.htm"
	},

	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	],

	"background": {
		"scripts": ["bg.js"]
	},

	"content_security_policy": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval'; object-src 'self'",
	"manifest_version": 2
}
